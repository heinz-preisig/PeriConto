digraph {
	B
	A
	B -> A [color=blue]
	string [color=green fillcolor=gray shape=none style=filled]
	string
	burl -> string [color=cyan]
	C
	A
	C -> A [color=blue]
	C
	curl
	C -> curl [color=black]
	string [color=green fillcolor=gray shape=none style=filled]
	string
	curl -> string [color=cyan]
	B
	burl
	B -> burl [color=black]
}
